source 'https://github.com/CocoaPods/Specs.git'
platform :ios, "10.0"

def main_pods
    pod 'AFNetworking', '~>1.3'
    pod 'SimulatorStatusMagic', :configurations => ['Debug']
end

target 'SleepMate' do
    main_pods
end

target 'SleepMate Tests' do
    main_pods
end

target 'SleepMateUITests' do
    main_pods
end
